  <div class="child-photo col-xs-12 col-sm-12 col-md-12 narrow">
    <a href={{this.link}} title="{{this.caption}}">
    {{#if this.thumbnail}}
      <img src={{this.thumbnail}} alt={{@key}} class="max-width project-photo">
    {{else}}
      <img src={{this.link}} alt={{@key}} class="max-width project-photo">
    {{/if}}
    </a>
  </div>